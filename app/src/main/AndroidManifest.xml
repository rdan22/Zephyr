<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zephyr.zephyrapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Zephyr"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.zephyr.zephyr.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.zephyr.zephyr.MainActivity" />
        <activity android:name="com.zephyr.zephyr.LoginActivity" />
        <activity android:name="com.zephyr.zephyr.SignupActivity" />
        <activity android:name="com.zephyr.zephyr.ProfileActivity"></activity>
    </application>

</manifest>